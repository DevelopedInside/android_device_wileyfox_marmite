# Glove Mode
genfscon proc /class/ms-touchscreen-msg20xx/device/glove_mode                         u:object_r:proc_glove_mode:s0

# Healthd
genfscon sysfs /devices/soc/78b6000.i2c/i2c-2/2-001c/power_supply                     u:object_r:sysfs_battery_supply:s0
genfscon sysfs /devices/soc/msm-bcl-19/power_supply                                   u:object_r:sysfs_battery_supply:s0
genfscon sysfs /devices/soc/soc:qcom,bcl/power_supply                                 u:object_r:sysfs_battery_supply:s0
genfscon sysfs /devices/soc/qpnp-smbcharger-17/power_supply                           u:object_r:sysfs_battery_supply:s0

# Power HAL
genfscon debugfs /wlan0                                                               u:object_r:debugfs_wlan:s0
genfscon debugfs /rmt_storage                                                         u:object_r:debugfs_rmt:s0

# USB
genfscon sysfs /sys/devices/soc/78db000.usb/msm_hsusb/gadget/lun0/                    u:object_r:sysfs_android_usb:s0
